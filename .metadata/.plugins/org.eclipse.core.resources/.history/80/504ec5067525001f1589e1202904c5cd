package com.ideasservice.dto;

import java.time.LocalDateTime;

import lombok.AllArgsConstructor;
import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;

@Getter
@Setter
@NoArgsConstructor
@AllArgsConstructor
@MappedSuperClass
public class GlobalResponseDto {

	String statusCode;

	String message;

	LocalDateTime time;

}
